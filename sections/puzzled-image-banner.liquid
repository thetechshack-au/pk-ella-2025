<link rel="stylesheet" href="{{ 'component-puzzled-image-banner.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'component-puzzled-image-banner.css' | asset_url | stylesheet_tag }}</noscript>

{%- liquid
    assign mg_top_desktop = section.settings.mg_top_desktop | append: 'px'
    assign mg_top_tablet = section.settings.mg_top_tablet | append: 'px'
    assign mg_top_mobile = section.settings.mg_top_mobile | append: 'px'

    assign mg_bottom_desktop = section.settings.mg_bottom_desktop | append: 'px'
    assign mg_bottom_tablet = section.settings.mg_bottom_tablet | append: 'px'
    assign mg_bottom_mobile = section.settings.mg_bottom_mobile | append: 'px'
    assign container = section.settings.container
    assign padding_full_width = section.settings.padding_full_width | append: 'px'

    assign section_heading = section.settings.section_heading
    assign section_heading_font_size = section.settings.section_heading_font_size | append: 'px'
    assign section_heading_font_size_mb = section.settings.section_heading_font_size_mb | append: 'px'
    assign section_heading_font_weight = section.settings.section_heading_font_weight
    assign section_heading_color = section.settings.section_heading_color
    assign section_heading_margin_bottom = section.settings.section_heading_margin_bottom | append: 'px'

    assign section_view_all = section.settings.section_view_all
    assign section_view_all_link = section.settings.section_view_all_link
    assign section_view_all_font_size = section.settings.section_view_all_font_size | append: 'px'
    assign section_view_all_font_weight = section.settings.section_view_all_font_weight
    assign section_view_all_color = section.settings.section_view_all_color
    assign section_view_all_margin_top = section.settings.section_view_all_margin_top | append: 'px'
    assign section_view_all_margin_bottom = section.settings.section_view_all_margin_bottom | append: 'px'

    assign block_width_1 = section.settings.block_width_1 | append: '%'
    assign image_1 = section.settings.image_1
    assign image_text_1 = section.settings.image_text_1
    assign image_text_1_font_size = section.settings.image_text_1_font_size | append: 'px'
    assign image_text_1_font_size_mb = section.settings.image_text_1_font_size_mb | append: 'px'
    assign image_text_1_font_weight = section.settings.image_text_1_font_weight
    assign image_text_1_color = section.settings.image_text_1_color
    assign image_link_1 = section.settings.image_link_1
    assign image_link_text_1 = section.settings.image_link_text_1
    assign image_link_1_font_size = section.settings.image_link_1_font_size | append: 'px'
    assign image_link_1_font_size_mb = section.settings.image_link_1_font_size_mb | append: 'px'
    assign image_link_1_font_weight = section.settings.image_link_1_font_weight
    assign image_link_1_color = section.settings.image_link_1_color

    assign block_width_2 = section.settings.block_width_2 | append: '%'
    assign image_2 = section.settings.image_2
    assign image_text_2 = section.settings.image_text_2
    assign image_text_2_font_size = section.settings.image_text_2_font_size | append: 'px'
    assign image_text_2_font_size_mb = section.settings.image_text_2_font_size_mb | append: 'px'
    assign image_text_2_font_weight = section.settings.image_text_2_font_weight
    assign image_text_2_color = section.settings.image_text_2_color
    assign image_link_2 = section.settings.image_link_2
    assign image_link_text_2 = section.settings.image_link_text_2
    assign image_link_2_font_size = section.settings.image_link_2_font_size | append: 'px'
    assign image_link_2_font_size_mb = section.settings.image_link_2_font_size_mb | append: 'px'
    assign image_link_2_font_weight = section.settings.image_link_2_font_weight
    assign image_link_2_color = section.settings.image_link_2_color

    assign block_width_3 = section.settings.block_width_3 | append: '%'
    assign image_3 = section.settings.image_3
    assign image_text_3 = section.settings.image_text_3
    assign image_text_3_font_size = section.settings.image_text_3_font_size | append: 'px'
    assign image_text_3_font_size_mb = section.settings.image_text_3_font_size_mb | append: 'px'
    assign image_text_3_font_weight = section.settings.image_text_3_font_weight
    assign image_text_3_color = section.settings.image_text_3_color
    assign image_link_3 = section.settings.image_link_3
    assign image_link_text_3 = section.settings.image_link_text_3
    assign image_link_3_font_size = section.settings.image_link_3_font_size | append: 'px'
    assign image_link_3_font_size_mb = section.settings.image_link_3_font_size_mb | append: 'px'
    assign image_link_3_font_weight = section.settings.image_link_3_font_weight
    assign image_link_3_color = section.settings.image_link_3_color
    
    assign block_width_4 = section.settings.block_width_4 | append: '%'
    assign image_4 = section.settings.image_4
    assign image_text_4 = section.settings.image_text_4
    assign image_text_4_font_size = section.settings.image_text_4_font_size | append: 'px'
    assign image_text_4_font_size_mb = section.settings.image_text_4_font_size_mb | append: 'px'
    assign image_text_4_font_weight = section.settings.image_text_4_font_weight
    assign image_text_4_color = section.settings.image_text_4_color
    assign image_link_4 = section.settings.image_link_4
    assign image_link_text_4 = section.settings.image_link_text_4
    assign image_link_4_font_size = section.settings.image_link_4_font_size | append: 'px'
    assign image_link_4_font_size_mb = section.settings.image_link_4_font_size_mb | append: 'px'
    assign image_link_4_font_weight = section.settings.image_link_4_font_weight
    assign image_link_4_color = section.settings.image_link_4_color

    assign content_spacing_top = section.settings.content_spacing_top | append: 'px'
    assign horizontal_spacing = section.settings.horizontal_spacing | append: 'px'
    assign vertical_spacing = section.settings.vertical_spacing | append: 'px'
    assign border_radius = section.settings.border_radius | append: 'px'
    assign show_shadow = section.settings.show_shadow
    assign shadow_color = section.settings.shadow_color
    assign view_all_position = section.settings.view_all_position
    assign block_right_top_spacing = section.settings.block_right_top_spacing | append: 'px'
    assign block_left_top_spacing = section.settings.block_left_top_spacing | append: 'px'
    assign left_block_width = section.settings.left_block_width | append: '%'
    assign right_block_width = 100 | minus: section.settings.left_block_width | append: '%'
    assign layout_mobile = section.settings.layout_mobile
-%}

<div class="puzzled-image-banner section-block-{{section.id}}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" id="puzzled-image-banner-{{section.id}}" data-loader-script="{{ 'puzzled-image-banner.js' | asset_url }}">
    <div class="{% if container == 'fullwidth' %}container-full{% elsif container == '1170' %}container-1170{% elsif container == '1770' %}container-1770{% else %}container{% endif %} text-center">
        {%- if section_heading != blank -%}
            <h2 class="section-heading title{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="--heading-font-size: {{ section_heading_font_size }};--heading-font-size-mb: {{ section_heading_font_size_mb }};--heading-font-weight: {{ section_heading_font_weight }};--heading-color: {{ section_heading_color }};--heading-margin-bottom: {{ section_heading_margin_bottom }}">{{ section_heading }}</h2>
            {%- if section_view_all != blank and view_all_position == "top" -%}
                <{% if section_view_all_link != blank %}a href="{{ section_view_all_link }}"{% else %}button type="button"{% endif %} class="view-all position-top section-view-all text-center{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="--view-all-font-size: {{ section_view_all_font_size }};--view-all-font-weight: {{ section_view_all_font_weight }};--view-all-color: {{ section_view_all_color }};--view-all-margin-top: {{ section_view_all_margin_top }};--view-all-margin-bottom: {{ section_view_all_margin_bottom }}"><span class="text">{{ section_view_all }}</span></{% if section_view_all_link != blank %}a{% else %}button{% endif %}>
            {%- endif -%}
        {%- endif -%}
        <{% if layout_mobile == 'scroll' %}div{%else%}puzzled-image-banner{%endif%} class="puzzled-image-banner-wrapper content-{{ section.settings.content_position }} content-{{ section.settings.content_alignment }}{% if section.settings.show_shadow %} show-shadow{% endif %} layout-{{ layout_mobile }}-mb" 
        style="--content_spacing_top: {{ content_spacing_top }};
        --column-gap:{{ horizontal_spacing }};
        --row-gap:{{ vertical_spacing }};
        --border_radius: {{ border_radius }};
        --shadow_color: {{ shadow_color }};
        --block_right_top_spacing: {{ block_right_top_spacing }};
        --block_left_top_spacing: {{ block_left_top_spacing }};
        --left_block_width: {{ left_block_width }};
        --right_block_width: {{ right_block_width }};
        ">
            <div class="content-block content-block-left">
                <image-to-flip class="section-block-banner-image{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: 1;{% endif %}--block-width: {{ block_width_1 }};">
                    <div class="img-box{% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} o-hidden{% endif %}">
                        {%- if image_1 != blank -%}
                            <a title="{{ image_1.alt | default: shop.name | escape }}" {% if image_link_1 != blank %}href="{{ image_link_1 }}"{% else %}role="link" aria-disabled="true"{% endif %} class="image-zoom adaptive_height image" style="padding-top: {% unless image_1.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: image_1.aspect_ratio | times: 120 | append: '%' }}{% else %}{{ 1 | divided_by: image_1.aspect_ratio | times: 100 | append: '%' }}{% endunless %}"><img {% render 'lazyload',preview_img: image_1 %}{% unless image_1.presentation.focal_point == '50.0% 50.0%' %} style="object-position: {{ image_1.presentation.focal_point }}"{% endunless %} />{%- if settings.enable_lazyload -%}<span class="data-lazy-loading"></span>{%-endif -%}</a>
                        {%- else -%}
                            <a {% if image_link_1 != blank %}href="{{ image_link_1 }}"{% else %}role="link" aria-disabled="true"{% endif %} class="image-zoom">
                                <div class="placeholder" style="--height_placeholder: clamp(350px, calc(1070 /1920 * 100vw), 1070px);">
                                    {{ 'hero-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
                                </div>
                            </a>
                        {%- endif -%}
                    </div>
                    {%- if image_text_1 != blank or image_link_1 != blank -%}
                        <div class="block-image-content">
                            {%- if image_text_1 != blank -%}
                                <h2 class="title image-title" style="--image-title-font-size: {{ image_text_1_font_size }};--image-title-font-size-mb: {{ image_text_1_font_size_mb }};--image-title-font-weight: {{ image_text_1_font_weight }};--image-title-color: {% if image_text_1_color != blank %}{{ image_text_1_color }}{% endif %}">{{ image_text_1 }}</h2>
                            {%- endif -%}
                            {%- if image_link_text_1 != blank -%}
                                <{% if image_link_1 != blank %}a href="{{ image_link_1 }}"{% else %}button type="button"{% endif %} class="link text-underline image-link{% if section.settings.image_link_1_font_size >= 30 %} fs_30{% endif %}" style="--link-color: {% if image_link_1_color != blank %}{{ image_link_1_color }}{% endif %};--link-font-size: {{ image_link_1_font_size }};--link-font-size-mb: {{ image_link_1_font_size_mb }}; --link-font-weight: {{ image_link_1_font_weight }}"><span class="text">{{ image_link_text_1 }} </span></{% if image_link_1 != blank %}a{% else %}button{% endif %}>
                            {%- endif -%}
                        </div>
                    {%- endif -%}
                </image-to-flip>
                <image-to-flip class="section-block-banner-image{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: 2;{% endif %}--block-width: {{ block_width_2 }};">
                    <div class="img-box{% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} o-hidden{% endif %}">
                        {%- if image_2 != blank -%}
                            <a title="{{ image_2.alt | default: shop.name | escape }}" {% if image_link_2 != blank %}href="{{ image_link_2 }}"{% else %}role="link" aria-disabled="true"{% endif %} class="image-zoom adaptive_height image" style="padding-top: {% unless image_2.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: image_2.aspect_ratio | times: 120 | append: '%' }}{% else %}{{ 1 | divided_by: image_2.aspect_ratio | times: 100 | append: '%' }}{% endunless %}"><img {% render 'lazyload',preview_img: image_2 %}{% unless image_2.presentation.focal_point == '50.0% 50.0%' %} style="object-position: {{ image_2.presentation.focal_point }}"{% endunless %} />{% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}</a>
                        {%- else -%}
                            <a {% if image_link_2 != blank %}href="{{ image_link_2 }}"{% else %}role="link" aria-disabled="true"{% endif %} class="image-zoom">
                                <div class="placeholder" style="--height_placeholder: clamp(350px, calc(1070 /1920 * 100vw), 1070px);">
                                    {{ 'hero-apparel-2' | placeholder_svg_tag: 'placeholder-svg' }}
                                </div>
                            </a>
                        {%- endif -%}
                    </div>
                    {%- if image_text_2 != blank or image_link_2 != blank -%}
                        <div class="block-image-content text-center">
                            {%- if image_text_2 != blank -%}
                                <h2 class="title image-title" style="--image-title-font-size: {{ image_text_2_font_size }};--image-title-font-size-mb: {{ image_text_2_font_size_mb }};--image-title-font-weight: {{ image_text_2_font_weight }};--image-title-color: {% if image_text_2_color != blank %}{{ image_text_2_color }}{% endif %}">{{ image_text_2 }}</h2>
                            {%- endif -%}
                            {%- if image_link_text_2 != blank -%}
                                <{% if image_link_2 != blank %}a href="{{ image_link_2 }}"{% else %}button type="button"{% endif %} class="link text-underline image-link{% if section.settings.image_link_2_font_size >= 30 %} fs_30{% endif %}" style="--link-color: {% if image_link_2_color != blank %}{{ image_link_2_color }}{% endif %};--link-font-size: {{ image_link_2_font_size }};--link-font-size-mb: {{ image_link_2_font_size_mb }};--link-font-weight: {{ image_link_2_font_weight }}"><span class="text">{{ image_link_text_2 }} </span></{% if image_link_2 != blank %}a{% else %}button{% endif %}>
                            {%- endif -%}
                        </div>
                    {%- endif -%}
                </image-to-flip>
            </div>
            <div class="content-block content-block-right">
                <image-to-flip class="section-block-banner-image{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: 1;{% endif %}--block-width: {{ block_width_3 }};">
                    <div class="img-box{% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} o-hidden{% endif %}">
                        {%- if image_3 != blank -%}
                            <a title="{{ image_3.alt | default: shop.name | escape }}" {% if image_link_3 != blank %}href="{{ image_link_3 }}"{% else %}role="link" aria-disabled="true"{% endif %} class="image-zoom adaptive_height image" style="padding-top: {% unless image_3.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: image_3.aspect_ratio | times: 120 | append: '%' }}{% else %}{{ 1 | divided_by: image_3.aspect_ratio | times: 100 | append: '%' }}{% endunless %}"><img {% render 'lazyload',preview_img: image_3 %}{% unless image_3.presentation.focal_point == '50.0% 50.0%' %} style="object-position: {{ image_3.presentation.focal_point }}"{% endunless %} />{% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}</a>
                        {%- else -%}
                            <a {% if image_link_3 != blank %}href="{{ image_link_3 }}"{% else %}role="link" aria-disabled="true"{% endif %} class="image-zoom">
                                <div class="placeholder" style="--height_placeholder: clamp(350px, calc(1070 /1920 * 100vw), 1070px);">
                                    {{ 'hero-apparel-2' | placeholder_svg_tag: 'placeholder-svg' }}
                                </div>
                            </a>
                        {%- endif -%}
                    </div>
                    {%- if image_text_3 != blank or image_link_3 != blank -%}
                        <div class="block-image-content text-center">
                            {%- if image_text_3 != blank -%}
                                <h2 class="title image-title" style="--image-title-font-size: {{ image_text_3_font_size }};--image-title-font-size-mb: {{ image_text_3_font_size_mb }};--image-title-font-weight: {{ image_text_3_font_weight }};--image-title-color: {% if image_text_3_color != blank %}{{ image_text_3_color }}{% endif %}">{{ image_text_3 }}</h2>
                            {%- endif -%}
                            {%- if image_link_text_3 != blank -%}
                                <{% if image_link_3 != blank %}a href="{{ image_link_3 }}"{% else %}button type="button"{% endif %} class="link text-underline image-link{% if section.settings.image_link_3_font_size >= 30 %} fs_30{% endif %}" style="--link-color: {% if image_link_3_color != blank %}{{ image_link_3_color }}{% endif %};--link-font-size: {{ image_link_3_font_size }};--link-font-size-mb: {{ image_link_3_font_size_mb }};--link-font-weight: {{ image_link_3_font_weight }}"><span class="text">{{ image_link_text_3 }} </span></{% if image_link_3 != blank %}a{% else %}button{% endif %}>
                            {%- endif -%}
                        </div>
                    {%- endif -%}
                </image-to-flip>
                <image-to-flip class="section-block-banner-image{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: 2;{% endif %}--block-width: {{ block_width_4 }};">
                    <div class="img-box{% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} o-hidden{% endif %}">
                        {%- if image_4 != blank -%}
                            <a title="{{ image_4.alt | default: shop.name | escape }}" {% if image_link_4 != blank %}href="{{ image_link_4 }}"{% else %}role="link" aria-disabled="true"{% endif %} class="image-zoom adaptive_height image" style="padding-top: {% unless image_4.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: image_4.aspect_ratio | times: 120 | append: '%' }}{% else %}{{ 1 | divided_by: image_4.aspect_ratio | times: 100 | append: '%' }}{% endunless %}"><img {% render 'lazyload',preview_img: image_4 %}{% unless image_4.presentation.focal_point == '50.0% 50.0%' %} style="object-position: {{ image_4.presentation.focal_point }}"{% endunless %} />{% if settings.enable_lazyload %}<span class="data-lazy-loading"></span>{% endif %}</a>
                        {%- else -%}
                            <a {% if image_link_4 != blank %}href="{{ image_link_4 }}"{% else %}role="link" aria-disabled="true"{% endif %} class="image-zoom">
                                <div class="placeholder" style="--height_placeholder: clamp(350px, calc(1070 /1920 * 100vw), 1070px);">
                                    {{ 'hero-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
                                </div>
                            </a>
                        {%- endif -%}
                    </div>
                    {%- if image_text_4 != blank or image_link_4 != blank -%}
                        <div class="block-image-content text-center">
                            {%- if image_text_4 != blank -%}
                                <h2 class="title image-title" style="--image-title-font-size: {{ image_text_4_font_size }};--image-title-font-size-mb: {{ image_text_4_font_size_mb }};--image-title-font-weight: {{ image_text_4_font_weight }};--image-title-color: {% if image_text_4_color != blank %}{{ image_text_4_color }}{% endif %}">{{ image_text_4 }}</h2>
                            {%- endif -%}
                            {%- if image_link_text_4 != blank -%}
                                <{% if image_link_4 != blank %}a href="{{ image_link_4 }}"{% else %}button type="button"{% endif %} class="link text-underline image-link{% if section.settings.image_link_4_font_size >= 30 %} fs_30{% endif %}" style="--link-color: {% if image_link_4_color != blank %}{{ image_link_4_color }}{% endif %};--link-font-size: {{ image_link_4_font_size }};--link-font-size-mb: {{ image_link_4_font_size_mb }};--link-font-weight: {{ image_link_4_font_weight }}"><span class="text">{{ image_link_text_4 }} </span></{% if image_link_4 != blank %}a{% else %}button{% endif %}>
                            {%- endif -%}
                        </div>
                    {%- endif -%}
                </image-to-flip>
                {%- if section_view_all != blank and view_all_position == "bottom" -%}
                    <{% if section_view_all_link != blank %}a href="{{ section_view_all_link }}"{% else %}button type="button"{% endif %} class="view-all section-view-all text-center mobile-hidden{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="--view-all-font-size: {{ section_view_all_font_size }};--view-all-font-weight: {{ section_view_all_font_weight }};--view-all-color: {{ section_view_all_color }};--view-all-margin-top: {{ section_view_all_margin_top }}"><span class="text">{{ section_view_all }}</span></{% if section_view_all_link != blank %}a{% else %}button{% endif %}>
                {%- endif -%}
            </div>
        </{% if layout_mobile == 'scroll' %}div{%else%}puzzled-image-banner{%endif%}>
        {%- if section_view_all != blank and view_all_position == "bottom" -%}
            <{% if section_view_all_link != blank %}a href="{{ section_view_all_link }}"{% else %}button type="button"{% endif %} class="view-all section-view-all text-center mobile-shown{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="--view-all-font-size: {{ section_view_all_font_size }};--view-all-font-weight: {{ section_view_all_font_weight }};--view-all-color: {{ section_view_all_color }};--view-all-margin-top: {{ section_view_all_margin_top }}"><span class="text">{{ section_view_all }}</span></{% if section_view_all_link != blank %}a{% else %}button{% endif %}>
        {%- endif -%}
    </div>
</div>

<style>
    @media (min-width: 1200px) {.section-block-{{section.id}} {padding-top: {{ mg_top_desktop }};padding-bottom: {{ mg_bottom_desktop }}}}
    {%- if section.settings.container == 'fullwidth' -%}
        @media (min-width: 1025px) {.section-block-{{section.id}} .container-full {padding-left: {{ padding_full_width }};padding-right: {{ padding_full_width }}}}
    {%- endif -%}
    @media (min-width: 768px) and (max-width: 1199px) {.section-block-{{ section.id }} {padding-top: {{ mg_top_tablet }};padding-bottom: {{ mg_bottom_tablet }}}}
    @media (max-width: 767px) {.section-block-{{ section.id }} {padding-top: {{ mg_top_mobile }};padding-bottom: {{ mg_bottom_mobile }}}}
</style>

{% schema %}
{
    "name": "t:sections.puzzled_image_banner.name",
    "settings": [
    {
        "type": "header",
        "content": "t:settings_schema.general.name"
    },
    {
        "type": "select",
        "id": "container",
        "label": "t:sections.layout.container.option",
        "default": "container",
        "options": [
        {
            "value": "container",
            "label": "t:sections.layout.container.option1"
        },
        {
            "value": "1170",
            "label": "t:sections.layout.container.option4"
        },
        {
            "value": "1770",
            "label": "t:sections.layout.container.option3"
        },
        {
            "value": "fullwidth",
            "label": "t:sections.layout.container.option2"
        }]
    },
    {
