{%- if section.blocks.size > 0 -%}
    {%-liquid
        assign autoplay = section.settings.autoplay
        assign autoplaySpeed = section.settings.slideshow_speed
        assign fade = section.settings.slideshow_transition
        assign dots =  section.settings.dots
        assign arrow =  section.settings.arrow_active
        assign container = section.settings.container
        assign border_radius = section.settings.border_radius
        assign check_lookbook = true
        if section.settings.dot_with_image and section.settings.dots_shape == 'disc' and dots == 'shape'
            assign show_dot_with_image = true
        else
            assign show_dot_with_image = false
        endif
        for block in section.blocks
        case block.type
            when 'video'
                assign has_video = true
        endcase
    endfor
    -%}

    <div class="slideshow-wrapper section-block-{{section.id}}" id="slideshow-wrapper-{{section.id}}" {% if section.blocks.size > 1 or has_video %}data-loader-script="{{ 'halo.slide-show.js' | asset_url }}"{% endif %} allow-transparent-header>
        <div class="{% if container == '1170' %}container-1170{% elsif section.settings.container == '1470' %}container-1470{% elsif section.settings.container == '1570' %}container-1570{% elsif container == '1770' %}container-1770{% elsif container == '1920' %}container-1920{% elsif container == 'fullwidth'%}container-full{% else %}container{% endif %}">
        {%- if section.settings.enable_categories or section.settings.enable_banner -%}
        <div class="wrapper-category-banner{% if section.settings.enable_categories %} has-categories{% endif %}{% if section.settings.enable_banner %} has-banner{% endif %}">
        {%- endif -%}
            {%- if section.settings.enable_categories -%}
                <div class="block-categories-slider">
                    <div class="wrapper-category">
                        <div class="title">
                            <span>{{ section.settings.text_categories }}</span>
                        </div>
                        <ul class="categories-slider--list disable-srollbar">
                            {% assign menu = linklists[section.settings.menu_category].links %}
                            {% for link in menu %}
                                <li>
                                    <a class="item-menu"  href="{{ link.url }}">
                                        {% assign img = link.handle | downcase | replace: ' ', '-' | append: '.png' | file_url %}
                                        <span class="icon-img image-zoom"><img src="{{ img }}" alt="{{ link.title }}" onError="style.display = 'none';" width="{{ img.width }}" height="{{ img.height }}"{% unless settings.enable_layout_rtl %}{% if settings.enable_lazyload %} loading="lazy"{% endif %}{% endunless %}/></span> <span class="text-menu">{{ link.title }}</span> </a>      
                                </li>
                            {% endfor %}
                        </ul>
                    </div>
                </div>
            {%- endif -%}
            <div class="slideshow{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--fade-in{% endif %}{% if show_dot_with_image %} show-dot-with-image{% endif %}" data-auto-video="{{ section.settings.auto_playvideo }}" {% if section.blocks.size > 1 %}data-init-slideshow data-arrows="{{arrow}}" data-dots="{{dots}}" data-autoplay="{{ autoplay }}" data-autoplay-speed="{{ autoplaySpeed }}" data-fade="{{ fade }}" {% endif %} data-dot-with-image="{% if show_dot_with_image %}true{% else %}false{% endif %}">
            {%- for block in section.blocks -%}
                {%- if block.type == 'image' or block.type == 'video' -%}
                    {%- if block.type == 'image' -%}
                        {%- liquid 
                            assign lookbook_style = section.settings.lookbook_style

                            assign dot_desktop_size = section.settings.dot_desktop_size | append: 'px'
                            assign dot_mobile_size = section.settings.dot_mobile_size | append: 'px'
                            assign dot_shadow_color = section.settings.dot_shadow_color | color_modify: 'alpha', 0.5
                            assign dot_outer_color = section.settings.dot_outer_color
                            assign show_dot_number = section.settings.show_dot_number
                            assign show_dot_number_mobile = section.settings.show_dot_number_mobile
                            assign dot_inner_color = section.settings.dot_inner_color
                            assign number_desktop_size = section.settings.number_desktop_size | append: 'px'
                            assign number_mobile_size = section.settings.number_mobile_size | append: 'px'
                            assign enable_lookbook_width = section.settings.enable_lookbook_width
                            assign lookbook_width = section.settings.lookbook_width | append: 'px'
                            assign lookbook_truncate = section.settings.lookbook_truncate
                            assign show_plus_icon = section.settings.show_plus_icon

                            assign product_1 = block.settings.lb_product_1
                            assign product_2 = block.settings.lb_product_2
                            assign product_3 = block.settings.lb_product_3

                            assign left_1 = block.settings.horizontal_pos_1 | append: '%' 
                            assign top_1 = block.settings.vertical_pos_1 | append: '%' 
                            assign lookbook_pos_1 = block.settings.lookbook_pos_1

                            assign left_2 = block.settings.horizontal_pos_2 | append: '%' 
                            assign top_2 = block.settings.vertical_pos_2 | append: '%' 
                            assign lookbook_pos_2 = block.settings.lookbook_pos_2

                            assign left_3 = block.settings.horizontal_pos_3 | append: '%' 
                            assign top_3 = block.settings.vertical_pos_3 | append: '%' 
                            assign lookbook_pos_3 = block.settings.lookbook_pos_3
                        -%}
                    {%- endif -%}
                <div class="item slide-block-{{block.id}}" id="block-{{block.id}}" data-index="{{forloop.index}}" data-style="{{ lookbook_style }}" data-show-lookbook-container{% if block.settings.image_for_dot != blank %} data-dot-img="{{ block.settings.image_for_dot | img_url: 'master' }}"{% endif %}>
                        {%- if block.type == 'image' -%}
                            {%- if check_lookbook -%}
                                {%- if product_1 != blank or product_2 != blank or product_3 != blank -%}
                                    {%- assign check_lookbook = false -%}
                                    <link rel="stylesheet" href="{{ 'slideshow-lookbook-style.css' | asset_url }}" media="print" onload="this.media='all'">
                                    <noscript>{{ 'slideshow-lookbook-style.css' | asset_url | stylesheet_tag }}</noscript>
                                    {{ 'lookbook-script.js' | asset_url | script_tag }}
                                {%- endif -%}
                            {%- endif -%}
                            <div class="images-contain lookbook-image-item {% if enable_lookbook_width %}width-restricted{% endif %}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--fade-in{% endif %}{% if section.settings.image_behavior != 'none' %} animate--{{ section.settings.image_behavior }} o-hidden{% endif %}" data-lookbook-item-container style="--dot-desktop-size: {{ dot_desktop_size }};--dot-mobile-size: {{ dot_mobile_size }};--dot-shadow-color: {{ dot_shadow_color }};--dot-outer-color: {{ dot_outer_color }};--dot-inner-color: {{ dot_inner_color }};--dot-number-desktop-size: {{ number_desktop_size }};--dot-number-mobile-size: {{ number_mobile_size }};--max-width: {{ lookbook_width }};--des-max-width: {% if des_width > 0 %}{{ des_width | append: 'px' }}{% else %}unset{% endif %};--truncate-line: {{ lookbook_truncate }}">
                                {% if block.settings.image_slide_mb != blank %}
                                    {%- assign img_url_mb = block.settings.image_slide_mb -%}
                                    <div class="adaptive_height image slide-mobile" style="padding-top: {% unless img_url_mb.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: img_url_mb.aspect_ratio | times: 120 }}%{% else %}{{ 1 | divided_by: img_url_mb.aspect_ratio | times: 100 }}%{% endunless %}">
                                        <a {% if block.settings.link == blank %}role="link" aria-disabled="true"{% else %}href="{{ block.settings.link }}"{% endif %} title="{{ img_url_mb.alt | default: shop.name | escape }}" class="slide-image">
                                            <img {% if settings.enable_lazyload and forloop.first == false %}data-{% endif %}srcset="{%- if img_url_mb.width < 320 -%}{{ img_url_mb | image_url: width: img_url_mb.width }} 320w,{%- endif -%}
                                                {%- if img_url_mb.width >= 320 -%}{{ img_url_mb | image_url: width: 375 }} 375w,{%- endif -%}
                                                {%- if img_url_mb.width >= 750 -%}{{ img_url_mb | image_url: width: 750 }} 750w,{%- endif -%}
                                                {%- if img_url_mb.width >= 1100 -%}{{ img_url_mb | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                {%- if img_url_mb.width >= 1370 -%}{{ img_url_mb | image_url: width: 1370 }} 1370w,{%- endif -%}
                                                {%- if img_url_mb.width >= 1500 -%}{{ img_url_mb | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                {%- if img_url_mb.width >= 1780 -%}{{ img_url_mb | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                {%- if img_url_mb.width >= 2000 -%}{{ img_url_mb | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                {%- if img_url_mb.width >= 2800 -%}{{ img_url_mb | image_url: width: 2800 }} 2800w{%- endif -%}"
                                                sizes="{% if section.settings.image != blank and section.settings.stack_images_on_mobile %}(min-width: 750px) 50vw, 100vw{% elsif section.settings.image != blank %}50vw{% else %}100vw{% endif %}" {% unless settings.enable_lazyload and forloop.first == false %}src="{{ img_url_mb | image_url: width: 750 }}"{% endunless %} alt="{{ img_url_mb.alt | escape }}" width="{{ img_url_mb.width }}" height="{{ img_url_mb.width | divided_by: img_url_mb.aspect_ratio | round}}"
                                                {%- if settings.enable_lazyload and forloop.first == false -%}class="lazyload"{% unless settings.enable_layout_rtl %} loading="lazy"{% endunless %}{%- endif -%}
                                                {% unless img_url_mb.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ img_url_mb.presentation.focal_point }}"{% endunless %}
                                            >
                                            {%- if settings.enable_lazyload and forloop.first == false -%}<span class="data-lazy-loading"></span>{%- endif -%}
                                        </a>
                                    </div>
                                {% endif %}
                                
                                {%- if block.settings.image_slide != blank -%}
                                    {%- assign img_url = block.settings.image_slide -%}
                                    <div class="adaptive_height image{% if block.settings.image_slide_mb != blank %} slide-pc{% endif %}" style="padding-top: {% unless img_url.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: img_url.aspect_ratio | times: 110 }}%; --padding-top-fpmb: {{ 1 | divided_by: img_url.aspect_ratio | times: 200 }}%{% else %}{{ 1 | divided_by: img_url.aspect_ratio | times: 100 }}%{% endunless %}">
                                        <a {% if block.settings.link == blank %}role="link" aria-disabled="true"{% else %}href="{{ block.settings.link }}"{% endif %} title="{{ img_url.alt | default: shop.name | escape }}" class="slide-image">
                                            <img {% if settings.enable_lazyload and forloop.first == false %}data-{% endif %}srcset="{%- if img_url.width < 320 -%}{{ img_url | image_url: width: img_url.width }} 320w,{%- endif -%}
                                                {%- if img_url.width >= 320 -%}{{ img_url | image_url: width: 375 }} 375w,{%- endif -%}
                                                {%- if img_url.width >= 740 -%}{{ img_url | image_url: width: 740 }} 740w,{%- endif -%} 
                                                {%- if img_url.width >= 750 -%}{{ img_url | image_url: width: 750 }} 750w,{%- endif -%}
                                                {%- if img_url.width >= 1100 -%}{{ img_url | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                {%- if img_url.width >= 1370 -%}{{ img_url | image_url: width: 1370 }} 1370w,{%- endif -%}
                                                {%- if img_url.width >= 1500 -%}{{ img_url | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                {%- if img_url.width >= 1770 -%}{{ img_url | image_url: width: 1770 }} 1770w,{%- endif -%}
                                                {%- if img_url.width >= 1780 -%}{{ img_url | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                {%- if img_url.width >= 1880 -%}{{ img_url | image_url: width: 1880 }} 1880w,{%- endif -%}
                                                {%- if img_url.width >= 2000 -%}{{ img_url | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                {%- if img_url.width >= 2800 -%}{{ img_url | image_url: width: 2800 }} 2800w{%- endif -%}"
                                                sizes="{% if section.settings.image != blank and section.settings.stack_images_on_mobile %}(min-width: 750px) 50vw, 100vw{% elsif section.settings.image != blank %}50vw{% else %}100vw{% endif %}" {% unless settings.enable_lazyload and forloop.first == false %}src="{{ img_url | image_url: width: 750 }}"{% endunless %} alt="{{ img_url.alt  | escape }}" width="{{ img_url.width }}" height="{{ img_url.width | divided_by: img_url.aspect_ratio | round }}" 
                                                {%- if settings.enable_lazyload and forloop.first == false -%}class="lazyload"{% unless settings.enable_layout_rtl %} loading="lazy"{% endunless %}{%- endif -%}
                                                {% unless img_url.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ img_url.presentation.focal_point }}"{% endunless %}
                                            >{%- if settings.enable_lazyload and forloop.first == false -%}<span class="data-lazy-loading"></span>{%- endif -%}</a>
                                    </div>
                                {%- else -%}
                                    <div class="placeholder{% if block.settings.image_slide_mb != blank %} slide-pc{% endif %}" style="--height_placeholder:{% if section.settings.enable_banner %} 510px{% else %} clamp(570px, calc(720 / 1920 * 100vw), 720px){% endif %};">
                                        {{ 'hero-apparel-2' | placeholder_svg_tag: 'placeholder-svg' }}
                                    </div>
                                {%- endif -%}

                                {%- if product_1 != blank or product_2 != blank or product_3 != blank -%}
                                    <lookbook-wrapper>
                                    {%- if product_1 != blank -%}
                                        {% render 'lookbook-modal-wrapper', lookbook_style: lookbook_style, top: top_1, left: left_1, show_dot_number: show_dot_number, show_dot_number_mobile: show_dot_number_mobile, lookbook_pos: lookbook_pos_1, product: product_1, show_plus_icon: show_plus_icon, num: 1 %}
                                    {% endif %}
                                    {%- if product_2 != blank -%}
                                        {% render 'lookbook-modal-wrapper', lookbook_style: lookbook_style, top: top_2, left: left_2, show_dot_number: show_dot_number, show_dot_number_mobile: show_dot_number_mobile, lookbook_pos: lookbook_pos_2, product: product_2, show_plus_icon: show_plus_icon, num: 2
                                        %}
                                    {%- endif -%}
                                    {%- if product_3 != blank -%}
                                        {% render 'lookbook-modal-wrapper', lookbook_style: lookbook_style, top: top_3, left: left_3, show_dot_number: show_dot_number, show_dot_number_mobile: show_dot_number_mobile, lookbook_pos: lookbook_pos_3, product: product_3, show_plus_icon: show_plus_icon, num: 3
                                        %}
                                    {%- endif -%}
                                    </lookbook-wrapper>
                                {%- endif -%}

                                <style type="text/css">
                                    #slideshow-wrapper-{{ section.id }} .lb-icon-1-{{ block.id }} .glyphicon {background-color: {{ block.settings.dots_bg_color_1 }}}
                                    #slideshow-wrapper-{{ section.id }} .lb-icon-1-{{ block.id }} .glyphicon:before,#slideshow-wrapper-{{ section.id }} .lb-icon-1-{{ block.id }} .glyphicon:after {background-color: {{ block.settings.dots_color_1 }}}
                                    #slideshow-wrapper-{{ section.id }} .lb-icon-1-{{ block.id }}:before{color: {{ block.settings.dots_shadow_color_1 | color_modify: 'alpha', 0.3 }}}
                                    #slideshow-wrapper-{{ section.id }} .lb-icon-2-{{ block.id }} .glyphicon {background-color: {{ block.settings.dots_bg_color_2 }}}
                                    #slideshow-wrapper-{{ section.id }} .lb-icon-2-{{ block.id }} .glyphicon:before,#slideshow-wrapper-{{ section.id }} .lb-icon-2-{{ block.id }} .glyphicon:after {background-color: {{ block.settings.dots_color_2 }}}
                                    #slideshow-wrapper-{{ section.id }} .lb-icon-2-{{ block.id }}:before{color: {{ block.settings.dots_shadow_color_2 | color_modify: 'alpha', 0.3 }}}
                                    #slideshow-wrapper-{{ section.id }} .lb-icon-3-{{ block.id }} .glyphicon {background-color: {{ block.settings.dots_bg_color_3 }}}
                                    #slideshow-wrapper-{{ section.id }} .lb-icon-3-{{ block.id }} .glyphicon:before,#slideshow-wrapper-{{ section.id }} .lb-icon-3-{{ block.id }} .glyphicon:after {
                                        background-color: {{ block.settings.dots_color_3 }}}
                                    #slideshow-wrapper-{{ section.id }} .lb-icon-3-{{ block.id }}:before{color: {{ block.settings.dots_shadow_color_3 | color_modify: 'alpha', 0.3}}}
                                </style>
                            </div>
                        {%- elsif block.type == 'video'-%}
                            {%- liquid
                                assign countdown = block.settings.countdown
                                assign font_size_countdown_number = block.settings.font_size_countdown_number
                                assign font_size_countdown_text = block.settings.font_size_countdown_text
                                assign color_countdown = block.settings.color_countdown
                                assign color_countdown_mb = block.settings.color_countdown_mb
                                assign countdown_margin_bottom = block.settings.countdown_margin_bottom
                                assign countdown_position = block.settings.countdown_position
                                assign autoplay_youtube = 0
                            -%}
                            {% if block.settings.style_video == 'youtube' %}
                                {%- liquid
                                    if forloop.first and section.settings.auto_playvideo
                                        assign autoplay_youtube = 1
                                    endif
                                -%}
                                <div data-video-youtube class="item slide-youtube embed-responsive item-video" id="slide-{{ block.id }}">
                                    <div class="fluid-width-video-wrapper">
                                        <iframe class="youtube-player{% if block.settings.video_link_mb != blank %} slide-pc{% endif %}" id="player_{{ block.id }}" width="100%" height="100%" data-video-id="{{ block.settings.video_link }}" src="https://www.youtube.com/embed/{{ block.settings.video_link }}?enablejsapi=1&rel=0&autoplay={{ autoplay_youtube }}&mute=1&loop=1&playlist={{ block.settings.video_link }}{% if block.settings.use_action_button %}&controls=0{% endif %}" frameborder="0" allowfullscreen=""></iframe>
                                        {%- if block.settings.video_link_mb != blank -%}
                                            <iframe class="youtube-player slide-mobile" id="player_mb_{{ block.id }}" width="100%" height="100%" data-video-id="{{ block.settings.video_link_mb }}" src="https://www.youtube.com/embed/{{ block.settings.video_link_mb }}?enablejsapi=1&rel=0&autoplay={{ autoplay_youtube }}&mute=1&loop=1&playlist={{ block.settings.video_link_mb }}{% if block.settings.use_action_button %}&controls=0{% endif %}" frameborder="0" allowfullscreen=""></iframe>
                                        {%- endif -%}
                                    </div>
                                </div>
                            {% else %}
                                <div data-video-mp4 class="item slide-video item-video" id="slide-{{ block.id }}">
                                    <div class="fluid-width-video-wrapper">
                                        <video class="video{% if block.settings.video_mp4_link_mb != blank %} slide-pc{% endif %}" muted playsinline preload="none" autoplay loop><source type="video/mp4" src="{{ block.settings.video_mp4_link }}"></video>
                                        {%- if block.settings.video_mp4_link_mb != blank -%}
                                            <video class="video slide-mobile" muted playsinline preload="none" autoplay loop><source type="video/mp4" src="{{ block.settings.video_mp4_link_mb }}"></video>
                                        {%- endif -%}
                                    </div>
                                </div>
                            {% endif %}
                            {% if block.settings.use_action_button %}
                            <div class="video-button {% if block.settings.style_video == 'youtube' and autoplay_youtube == 0 %}pause{% else %}play{% endif %}">
                                {% render 'play-button' %}
                                {% render 'pause-button' %}
                            </div>
                            <style>
                                .section-block-{{ section.id }} .video-button {position: absolute;top: {{ block.settings.button_vertical | append: '%' }};left: {{ block.settings.button_horizontal | append: '%' }};transform: translate({{ block.settings.button_horizontal | times: -1 | append: '%' }}, {{ block.settings.button_vertical | times: -1 | append: '%' }}) !important;}
                                .section-block-{{ section.id }} .video-button svg {display:none;fill: {{ block.settings.button_color }};stroke: {{ block.settings.button_color }};width: 86px;height: 86px;}
                                .section-block-{{ section.id }} .video-button:hover svg {fill: {{ block.settings.button_color_hover }};stroke: {{ block.settings.button_color_hover }};cursor: pointer;}
                                .section-block-{{ section.id }} .video-button.pause svg.pause-button {display: block;}
                                .section-block-{{ section.id }} .video-button.play svg.play-button {display: block;}
                                .section-block-{{ section.id }} .item:has(.video-button) iframe {pointer-events: none;}
                                @media(max-width: 1024px){
                                    .section-block-{{ section.id }} .video-button svg {width: 64.5px;height: 64.5px;}
                                }
                                @media(max-width: 767px){
                                    .section-block-{{ section.id }} .video-button svg {width: 43px;height: 43px;}
                                }
                                @media(max-width: 550px){
                                    .section-block-{{ section.id }} .slide-content .countdown-block{justify-content: center;}
                                }
                            </style>
                            {% endif %}
                        {%- endif -%}
                        {%- if block.settings.heading != blank or block.settings.slide-des != blank or block.settings.button_slide != blank or section.settings.dots_shape == 'line' -%}
                            <div class="slide-content content-box--absolute content-box--{{ block.settings.alignment }} text-{{ block.settings.text_alignment }}{% if block.settings.text_alignment_mb != blank %} text-mb-{{ block.settings.text_alignment_mb }}{% endif %} {% if block.settings.enable_custom_position %}custom_position{% endif %} {% if block.settings.enable_position_outside_image %}enable_position_outside_image{% endif %}{% if block.settings.sub_heading_font == 'font_1' %} sub_heading_font_1{% endif %}" {% if block.settings.enable_custom_position %}style="--horizontal_position: {{ block.settings.custom_horizontal }}%;--vertical_position: {{ block.settings.custom_vertical }}%;{% if block.settings.custom_vertical_mb != blank %}--vertical_position_mb: {{ block.settings.custom_vertical_mb }}%;{% endif %}--minus_horizontal_position: -{{ block.settings.custom_horizontal }}%;--minus_vertical_position: -{{ block.settings.custom_vertical }}%"{% endif %}>
                            {%- if block.settings.profile_name != blank or block.settings.profile_title != blank or block.settings.profile_picture != blank -%}
                                <div class="slide-content__person slide-testimonial{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
                                    {%- if block.settings.profile_picture != blank -%}
                                        <img class="slide-content__person profile-image" src="{{ block.settings.profile_picture | image_url: width: 720, height: 720 }}" width={{ block.settings.profile_picture.width }} height={{ block.settings.profile_picture.height }} {% unless settings.enable_layout_rtl %}{% if settings.enable_lazyload %} loading="lazy"{% endif %}{% endunless %} />
                                    {%- else -%}
                                        <div class="slide-content__person profile-image empty"></div>
                                    {%- endif -%}
                                    {% if block.settings.profile_name != blank or block.settings.profile_title %}
                                        <div class="slide-content__person profile-name-and-title">
                                            {%- if block.settings.profile_name != blank -%}
                                                <h4 class="slide-content__person profile-name"><span>{{ block.settings.profile_name }}</span></h4>
                                            {%- endif -%}
                                            {%- if block.settings.profile_title != blank -%}
                                                <p class="slide-content__person profile-title"><span>{{ block.settings.profile_title }}</span></p>
                                            {%- endif -%}
                                        </div>
                                    {% endif %}
                                </div>
                            {%- endif -%}
                            {%- if block.settings.sub_title_position == 'sub_title_above' -%}
                                {%- if block.settings.sub_heading != blank or block.settings.sub_heading_mb != blank -%}
                                    {%- if block.settings.sub_heading != blank -%}
                                    <h3 class="slide-sub slide-pc border--{{ block.settings.border_sub_heading }}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" style="{% if settings.banner_animation == 'effect_fade_up' %}--animation-order: 0;{% endif %}--slide-sub-font-size-style: {{ block.settings.font_size_sub_heading | append: 'px' }}; --slide-sub-color-style: {{ block.settings.color_sub_heading }}; --sub-heading-spacing-bottom-style: {{ block.settings.sub_heading_spacing_bottom | append: 'px' }}; --font-weight-sub-heading-style: {{ block.settings.font_weight_sub_heading }}; --font-sub-heading-type:{%- if block.settings.sub_heading_font == 'font_1' -%}var(--font-tab-type-1){%- else -%}var(--font-tab-type-2){%- endif -%}"><span>{{ block.settings.sub_heading }}</span></h3>
                                    {%- endif -%}
                                    {%- if block.settings.sub_heading_mb != blank -%}
