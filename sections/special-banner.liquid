{{ 'component-special-banner.css' | asset_url | stylesheet_tag }}

{%- liquid 

    assign section_bg = section.settings.section_bg
    assign section_bg_gradient = section.settings.section_bg_gradient
    assign section_bg_image = section.settings.section_bg_image

    assign mg_top_desktop = section.settings.mg_top_desktop
    assign mg_top_tablet = section.settings.mg_top_tablet
    assign mg_top_mobile = section.settings.mg_top_mobile

    assign mg_bottom_desktop = section.settings.mg_bottom_desktop
    assign mg_bottom_tablet = section.settings.mg_bottom_tablet
    assign mg_bottom_mobile = section.settings.mg_bottom_mobile
-%}

<style>
    .section-block-{{section.id}} {padding-top: {{ mg_top_mobile | append: 'px' }};padding-bottom: {{ mg_bottom_mobile | append: 'px' }}}
    @media (min-width: 768px) and (max-width: 1199px) {.section-block-{{section.id}} {padding-top: {{ mg_top_tablet | append: 'px' }};padding-bottom: {{ mg_bottom_tablet | append: 'px' }}}}
    @media (min-width: 1200px) {.section-block-{{section.id}} {padding-top: {{ mg_top_desktop | append: 'px' }};padding-bottom: {{ mg_bottom_desktop | append: 'px' }}}}

    {%- if section_bg_image != blank %}
        .section-block-{{section.id}}{background: url({{ section_bg_image | img_url: '1920x' }});background-repeat: repeat-x;background-size: cover}
    {%- else -%}
        .section-block-{{section.id}}{background: {% if section_bg_gradient != blank %}{{ section_bg_gradient }}{% else %}{{ section_bg }}{% endif %}}
    {%- endif -%}
</style>

<div class="special-banner section-block-{{section.id}}" style="--p-top: {{ section.settings.padding_top }}px; --p-bottom: {{ section.settings.padding_bottom }}px">
    <div class="container-{{ section.settings.container }}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
        <div class="special-banner__wrapper">
            {%- for block in section.blocks -%}
                {%-  case block.type  -%}
                    {%- when 'image_rotating_text' -%}
                        {%-  liquid
                            assign image = block.settings.image
                            assign rotating_text = block.settings.rotating_text
                            assign link = block.settings.link
                        -%}
                        {%- if image != blank or rotating_text != blank -%}
                            <div class="special-banner__item special-banner__item--{{ block.type }}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}" sticky-special-banner>
                                <div class="special-banner__img_box{% if block.settings.image_behavior != 'none' %} animate--{{ block.settings.image_behavior }} o-hidden{% endif %}">
                                    <div class="special-banner__img image-zoom adaptive_height image" style="padding-top: {% if image != blank %}{% unless image.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: image.aspect_ratio | times: 120 }}%{% else %}{{ 1 | divided_by: image.aspect_ratio | times: 100 }}%{% endunless %}{% endif %}">
                                        {%- if image != blank -%}
                                            <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{%- if image.width < 320 -%}{{ image | image_url: width: image.width }} 320w,{%- endif -%}
                                                {%- if image.width >= 320 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
                                                {%- if image.width >= 750 -%}{{ image | image_url: width: 750 }} 750w,{%- endif -%}
                                                {%- if image.width >= 1100 -%}{{ image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                {%- if image.width >= 1370 -%}{{ image | image_url: width: 1370 }} 1370w,{%- endif -%}
                                                {%- if image.width >= 1500 -%}{{ image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                {%- if image.width >= 1780 -%}{{ image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                {%- if image.width >= 2000 -%}{{ image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                {%- if image.width >= 2800 -%}{{ image | image_url: width: 2800 }} 2800w{%- endif -%}"
                                                sizes="{% if section.settings.image != blank and section.settings.stack_images_on_mobile %}(min-width: 750px) 50vw, 100vw{% elsif section.settings.image != blank %}50vw{% else %}100vw{% endif %}" {% unless settings.enable_lazyload %}src="{{ image | image_url: width: 750 }}"{% endunless %} alt="{{ image.alt | escape }}" width="{{ image.width }}" height="{{ image.width | divided_by: image.aspect_ratio | round}}"
                                                {%- if settings.enable_lazyload -%}class="lazyload" {% unless settings.enable_layout_rtl %} loading="lazy"{% endunless %}{%- endif -%}
                                                {% unless image.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ image.presentation.focal_point }}"{% endunless %}
                                            >
                                            {%- if settings.enable_lazyload -%}<span class="data-lazy-loading"></span>{%- endif -%}
                                        {%- else -%}
                                            <div class="placeholder" style="--height_placeholder: clamp(550px, calc(1070 /1920 * 100vw), 1070px);">
                                                {{ 'hero-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
                                            </div>
                                        {%- endif -%}
                                        {%- if rotating_text != blank -%}
                                            <div class="special-banner__rotatingText position--{{ block.settings.position }}" style="--color: {{ block.settings.rotating_text_color }}; --letter-spacing: {{ block.settings.letter_spacing }}px">
                                                <svg class="rotatingText" viewBox="0 0 200 200"><defs><path id="circlePath" d=" M 100, 100 m -75, 0 a 75,75 0 0,1 150,0 a 75,75 0 0,1 -150,0 "/></defs><circle cx="150" cy="100" r="75" fill="none"/><g><use xlink:href="#circlePath" fill="none"/><text><textPath xlink:href="#circlePath">{{ rotating_text }}</textPath></text></g></svg>
                                            </div>
                                        {%- endif -%}
                                        {%- if link != blank-%}
                                            <a href="{{ link }}" title="{{ image.alt | default: shop.name | escape }}"></a>
                                        {%- endif -%}
                                    </div>
                                </div>
                            </div>
                        {%- endif -%}
                    {%- when 'image_with_text' -%}
                        {%-  liquid
                            assign image = block.settings.image
                            assign link = block.settings.link
                            assign heading = block.settings.heading
                            assign desc = block.settings.desc
                            assign btn_txt = block.settings.btn_txt
                            assign content_align = block.settings.content_align
                        -%}
                        {%- if image != blank or heading != blank or desc != blank or btn_txt != blank -%}
                            <div class="special-banner__item special-banner__item--{{ block.type }}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger {% if settings.animations_hover_elements == '3d-lift' %}animate--fade-in{% else %}animate--slide-in{% endif %}{% endif %}{% if block.settings.image_behavior != 'none' %} animate--{{ block.settings.image_behavior }} o-hidden{% endif %}" sticky-special-banner>
                                <div class="special-banner__img p-relative adaptive_height image-zoom image"{% if image != blank %} style="padding-top: {% if image != blank %}{% unless image.presentation.focal_point == '50.0% 50.0%' %}{{ 1 | divided_by: image.aspect_ratio | times: 120 }}%{% else %}{{ 1 | divided_by: image.aspect_ratio | times: 100 }}%{% endunless %}{% else %}100%{% endif %}"{% endif %}>
                                    {%- if image != blank -%}
                                        <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{%- if image.width < 320 -%}{{ image | image_url: width: image.width }} 320w,{%- endif -%}
                                            {%- if image.width >= 320 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
                                            {%- if image.width >= 750 -%}{{ image | image_url: width: 750 }} 750w,{%- endif -%}
                                            {%- if image.width >= 1100 -%}{{ image | image_url: width: 1100 }} 1100w,{%- endif -%}
                                            {%- if image.width >= 1370 -%}{{ image | image_url: width: 1370 }} 1370w,{%- endif -%}
                                            {%- if image.width >= 1500 -%}{{ image | image_url: width: 1500 }} 1500w,{%- endif -%}
                                            {%- if image.width >= 1780 -%}{{ image | image_url: width: 1780 }} 1780w,{%- endif -%}
                                            {%- if image.width >= 2000 -%}{{ image | image_url: width: 2000 }} 2000w,{%- endif -%}
                                            {%- if image.width >= 2800 -%}{{ image | image_url: width: 2800 }} 2800w{%- endif -%}"
                                            sizes="{% if section.settings.image != blank %}(min-width: 750px) 50vw, 100vw{% elsif section.settings.image != blank %}50vw{% else %}100vw{% endif %}" {% unless settings.enable_lazyload %}src="{{ image | image_url: width: 750 }}"{% endunless %} alt="{{ image.alt | escape }}" width="{{ image.width }}" height="{{ image.width | divided_by: image.aspect_ratio | round}}"
                                            {%- if settings.enable_lazyload -%}class="lazyload" {% unless settings.enable_layout_rtl %} loading="lazy"{% endunless %}{%- endif -%}
                                            {% unless image.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ image.presentation.focal_point }}"{% endunless %}
                                        >
                                        {%- if settings.enable_lazyload -%}<span class="data-lazy-loading"></span>{%- endif -%}
                                    {%- else -%}
                                        <div class="placeholder" style="--height_placeholder: clamp(550px, calc(1070 /1920 * 100vw), 1070px);">
                                            {{ 'hero-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
                                        </div>
                                    {%- endif -%}
                                    {%- if heading != blank or desc != blank or btn_txt != blank -%}
                                        <div class="special-banner__content p-absolute left-0 right-0 text-{{block.settings.text_align}}{% if content_align == 'top' %} special-banner__content_top top-0{% elsif content_align == 'center' %} special-banner__content_center middle-y{% else %} special-banner__content_bottom bottom-0{% endif %}" style="--text-color: {{ block.settings.text_color }}">
                                            {%- if heading != blank -%}
                                                <h3 class="heading m-zero mb-15" style="--font-size: {{block.settings.fontsize_heading | append: 'px'}};--font-size-mb: {{block.settings.fontsize_heading_mb | append: 'px'}};--line-height: {{block.settings.fontsize_heading | plus: 8 | append: 'px'}}; --line-height-mb: {{block.settings.fontsize_heading_mb | plus: 8 | append: 'px'}}">{{heading}}</h3>
                                            {%- endif -%}
                                            {%- if desc != blank -%}
                                                <div class="desc d-inline-block w-100">{{desc}}</div>
                                            {%- endif -%}
                                            {%- if btn_txt != blank -%}
                                                <span class="button_link d-inline-block txt-d-underline{% if block.settings.fontsize_btn <= 18 %} txt-u-o-6{% else %} txt-u-o-8{% endif %}" style="--font-size-btn: {{block.settings.fontsize_btn | append: 'px'}}">{{btn_txt}}</span>
                                            {%- endif -%}
                                        </div>
                                    {%- endif -%}
                                    {%- if link != blank-%}
                                        <a href="{{ link }}" title="{{ image.alt | default: shop.name | escape }}"></a>
                                    {%- endif -%}
                                </div>
                            </div>
                        {%- endif -%}
                    {%- when 'collection_banner' -%}
                        <div class="special-banner__item special-banner__item--{{ block.type }} d-flex flex-jc-center flex-align-center p-relative{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
                            <div class="special-banner__collection d-flex" style="--font-size: {{block.settings.fontsize_heading | append: 'px'}};--font-size-mb: {{block.settings.fontsize_heading_mb | append: 'px'}};--line-height: {{block.settings.fontsize_heading | plus: 8 | append: 'px'}}; --line-height-mb: {{block.settings.fontsize_heading_mb | plus: 8 | append: 'px'}};--font-size-btn: {{block.settings.fontsize_btn | append: 'px'}}">
                                {%-  liquid
                                    assign image_1 = block.settings.image_1
                                    assign image_2 = block.settings.image_2
                                -%}
                                <div class="special-banner__coll_item special-banner__coll_item_1 d-inline-block w-50pc image-zoom{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
                                    {%- if image_1 != blank -%}
                                        {%- if block.settings.btn_lnk_1 != blank -%}<a href="{{block.settings.btn_lnk_1}}" class="link">{%- endif -%}
                                        <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{%- if image_1.width < 320 -%}{{ image_1 | image_url: width: image_1.width }} 320w,{%- endif -%}
                                            {%- if image_1.width >= 320 -%}{{ image_1 | image_url: width: 375 }} 375w,{%- endif -%}
                                            {%- if image_1.width >= 750 -%}{{ image_1 | image_url: width: 750 }} 750w,{%- endif -%}
                                            {%- if image_1.width >= 1100 -%}{{ image_1 | image_url: width: 1100 }} 1100w,{%- endif -%}
                                            {%- if image_1.width >= 1370 -%}{{ image_1 | image_url: width: 1370 }} 1370w,{%- endif -%}
                                            {%- if image_1.width >= 1500 -%}{{ image_1 | image_url: width: 1500 }} 1500w,{%- endif -%}
                                            {%- if image_1.width >= 1780 -%}{{ image_1 | image_url: width: 1780 }} 1780w,{%- endif -%}
                                            {%- if image_1.width >= 2000 -%}{{ image_1 | image_url: width: 2000 }} 2000w,{%- endif -%}
                                            {%- if image_1.width >= 2800 -%}{{ image_1 | image_url: width: 2800 }} 2800w{%- endif -%}"
                                            sizes="50vw"{% unless settings.enable_lazyload %} src="{{ image_1 | image_url: width: 750 }}"{% endunless %} alt="{{ image_1.alt | escape }}" width="{{ image_1.width }}" height="{{ image_1.width | divided_by: image_1.aspect_ratio | round}}" 
                                            {%- if settings.enable_lazyload -%}class="lazyload" {% unless settings.enable_layout_rtl %} loading="lazy"{% endunless %}{%- endif -%}
                                            {% unless image_1.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ image_1.presentation.focal_point }}"{% endunless %}
                                        >
                                    {%- if block.settings.btn_lnk_1 != blank -%}</a>{%- endif -%}
                                        {%- if settings.enable_lazyload -%}<span class="data-lazy-loading"></span>{%- endif -%}
                                    {%- else -%}
                                        <div class="placeholder" style="--height_placeholder: 450px;">
                                            {{ 'product-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
                                        </div>
                                    {%- endif -%}
                                    {%- if block.settings.heading_1 != blank -%}
                                    <h3 class="heading mt-25 mb-20{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">{%- if block.settings.btn_lnk_1 != blank -%}<a href="{{block.settings.btn_lnk_1}}" class="link-effect p-relative">{%- endif -%}<span class="text">{{ block.settings.heading_1 }}</span>{%- if block.settings.btn_lnk_1 != blank -%}</a>{%- endif -%}</h3>
                                    {%- endif -%}
                                    {%- if block.settings.desc_1 != blank -%}
                                    <div class="desc mb-20{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">{{block.settings.desc_1}}</div>
                                    {%- endif -%}
                                    {%- if block.settings.btn_txt_1 != blank -%}
                                        {%- if block.settings.btn_lnk_1 != blank -%}
                                            <a href="{{block.settings.btn_lnk_1}}" class="button_link coll_item_btn d-inline-block txt-d-underline{% if block.settings.fontsize_btn <= 18 %} txt-u-o-8{% else %} txt-u-o-12{% endif %}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">{{block.settings.btn_txt_1}}</a>
                                        {%- else -%}
                                            <span class="coll_item_btn">{{block.settings.btn_txt_1}}</span>
                                        {%- endif -%}
                                    {%- endif -%}
                                </div>
                                <div class="special-banner__coll_item special-banner__coll_item_2 d-inline-block w-50pc image-zoom{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
                                    {%- if image_2 != blank -%}
                                        {%- if block.settings.btn_lnk_2 != blank -%}<a href="{{block.settings.btn_lnk_2}}" class="link">{%- endif -%}
                                        <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{%- if image_2.width < 320 -%}{{ image_2 | image_url: width: image_2.width }} 320w,{%- endif -%}
                                            {%- if image_2.width >= 320 -%}{{ image_2 | image_url: width: 375 }} 375w,{%- endif -%}
                                            {%- if image_2.width >= 750 -%}{{ image_2 | image_url: width: 750 }} 750w,{%- endif -%}
                                            {%- if image_2.width >= 1100 -%}{{ image_2 | image_url: width: 1100 }} 1100w,{%- endif -%}
                                            {%- if image_2.width >= 1370 -%}{{ image_2 | image_url: width: 1370 }} 1370w,{%- endif -%}
                                            {%- if image_2.width >= 1500 -%}{{ image_2 | image_url: width: 1500 }} 1500w,{%- endif -%}
                                            {%- if image_2.width >= 1780 -%}{{ image_2 | image_url: width: 1780 }} 1780w,{%- endif -%}
                                            {%- if image_2.width >= 2000 -%}{{ image_2 | image_url: width: 2000 }} 2000w,{%- endif -%}
                                            {%- if image_2.width >= 2800 -%}{{ image_2 | image_url: width: 2800 }} 2800w{%- endif -%}"
                                            sizes="50vw"{% unless settings.enable_lazyload %} src="{{ image_2 | image_url: width: 320 }}"{% endunless %} alt="{{ image_2.alt | escape }}" width="{{ image_2.width }}" height="{{ image_2.width | divided_by: image_2.aspect_ratio | round}}" class="lazyload" 
                                            {%- if settings.enable_lazyload -%}class="lazyload" {% unless settings.enable_layout_rtl %} loading="lazy"{% endunless %}{%- endif -%}
                                            {% unless image_2.presentation.focal_point == '50.0% 50.0%' %}style="object-position: {{ image_2.presentation.focal_point }}"{% endunless %}
                                        >
                                        {%- if block.settings.btn_lnk_2 != blank -%}</a>{%- endif -%}
                                        {%- if settings.enable_lazyload -%}<span class="data-lazy-loading"></span>{%- endif -%}
                                    {%- else -%}
                                        <div class="placeholder" style="--height_placeholder: 450px;">
                                            {{ 'product-apparel-2' | placeholder_svg_tag: 'placeholder-svg' }}
                                        </div>
                                    {%- endif -%}
                                    {%- if block.settings.heading_2 != blank -%}
                                    <h3 class="heading mt-25 mb-20{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">{%- if block.settings.btn_lnk_2 != blank -%}<a href="{{block.settings.btn_lnk_2}}" class="link-effect p-relative">{%- endif -%}<span class="text">{{ block.settings.heading_2 }}</span>{%- if block.settings.btn_lnk_2 != blank -%}</a>{%- endif -%}</h3>
                                    {%- endif -%}
                                    {%- if block.settings.desc_2 != blank -%}
                                    <div class="desc mb-20{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">{{block.settings.desc_2}}</div>
                                    {%- endif -%}
                                    {%- if block.settings.btn_txt_2 != blank -%}
                                        {%- if block.settings.btn_lnk_2 != blank -%}
                                            <a href="{{block.settings.btn_lnk_2}}" class="button_link coll_item_btn d-inline-block txt-d-underline{% if block.settings.fontsize_btn <= 18 %} txt-u-o-8{% else %} txt-u-o-12{% endif %}{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">{{block.settings.btn_txt_2}}</a>
                                        {%- else -%}
                                            <span class="coll_item_btn">{{block.settings.btn_txt_2}}</span>
                                        {%- endif -%}
                                    {%- endif -%}
                                </div>
                            </div>
                        </div>
                    {%- when 'lookbook_banner' -%}
                        {%-  liquid 
                            assign image_1 = block.settings.image_1
                            assign image_2 = block.settings.image_2
                            assign image_3 = block.settings.image_3
                            assign product_1_1 = block.settings.lb_product_1_1
                            assign left_1_1 = block.settings.horizontal_pos_1_1 | append: '%' 
                            assign top_1_1 = block.settings.vertical_pos_1_1 | append: '%'

                            assign product_1_2 = block.settings.lb_product_1_2
                            assign left_1_2 = block.settings.horizontal_pos_1_2 | append: '%' 
                            assign top_1_2 = block.settings.vertical_pos_1_2 | append: '%'

                            assign product_2_1 = block.settings.lb_product_2_1
                            assign left_2_1 = block.settings.horizontal_pos_2_1 | append: '%' 
                            assign top_2_1 = block.settings.vertical_pos_2_1 | append: '%'

                            assign product_2_2 = block.settings.lb_product_2_2
                            assign left_2_2 = block.settings.horizontal_pos_2_2 | append: '%' 
                            assign top_2_2 = block.settings.vertical_pos_2_2 | append: '%'

                            assign product_3_1 = block.settings.lb_product_3_1
                            assign left_3_1 = block.settings.horizontal_pos_3_1 | append: '%' 
                            assign top_3_1 = block.settings.vertical_pos_3_1 | append: '%'

                            assign product_3_2 = block.settings.lb_product_3_2
                            assign left_3_2 = block.settings.horizontal_pos_3_2 | append: '%' 
                            assign top_3_2 = block.settings.vertical_pos_3_2 | append: '%'

                            assign lookbook_style = block.settings.lookbook_style
                            assign check_lookbook = true
                        -%}
                        <div class="special-banner__item special-banner__item--{{ block.type }} d-flex flex-jc-center flex-align-center p-relative{% if settings.banner_animation == 'effect_fade_up' %} scroll-trigger animate--slide-in{% endif %}">
                            {%- if check_lookbook -%}
                                {%- if product_1_1 != blank or product_1_2 != blank or product_2_1 != blank or product_2_2 != blank or product_3_1 != blank or product_3_2 != blank -%}
                                    {%- assign check_lookbook = false -%}
                                    <script src="{{ 'lookbook-script.js' | asset_url }}" defer="defer"></script>
                                {%- endif -%}
                            {%- endif -%}
                            <div class="special-banner__lookbook lookbook-carousel{% if block.settings.lookbook_counter_number %} enable_counter_number{% endif %}" style="--font-size: {{block.settings.fontsize_heading | append: 'px'}};--font-size-mb: {{block.settings.fontsize_heading_mb | append: 'px'}};--line-height: {{block.settings.fontsize_heading | plus: 8 | append: 'px'}}; --line-height-mb: {{block.settings.fontsize_heading_mb | plus: 8 | append: 'px'}};--icon_color: {{block.settings.icon_color}}" data-item-to-show="1" data-item-dots="false" data-item-dots-mb="true" data-item-arrows="true" data-item-arrows-mb="false">
                                <div class="special-banner__lb_item special-banner__lb_item_1 d-inline-block w-100{% if block.settings.image_behavior != 'none' %} animate--{{ block.settings.image_behavior }} o-hidden{% endif %}">
                                    {%- if image_1 != blank -%}
                                    {% if settings.animations_hover_elements == '3d-lift' or settings.animations_hover_elements == 'vertical-lift' %}
                                    <div class="image-animation">
                                    {% endif %}
                                        <div class="lookbook-img p-relative image-zoom image">
                                            {%- if block.settings.lnk_1 != blank -%}<a href="{{block.settings.lnk_1}}" class="link">{%- endif -%}
                                            <img {% if settings.enable_lazyload %}data-{% endif %}srcset="{%- if image_1.width < 320 -%}{{ image_1 | image_url: width: image_1.width }} 320w,{%- endif -%}
                                                {%- if image_1.width >= 320 -%}{{ image_1 | image_url: width: 375 }} 375w,{%- endif -%}
                                                {%- if image_1.width >= 750 -%}{{ image_1 | image_url: width: 750 }} 750w,{%- endif -%}
                                                {%- if image_1.width >= 1100 -%}{{ image_1 | image_url: width: 1100 }} 1100w,{%- endif -%}
                                                {%- if image_1.width >= 1370 -%}{{ image_1 | image_url: width: 1370 }} 1370w,{%- endif -%}
                                                {%- if image_1.width >= 1500 -%}{{ image_1 | image_url: width: 1500 }} 1500w,{%- endif -%}
                                                {%- if image_1.width >= 1780 -%}{{ image_1 | image_url: width: 1780 }} 1780w,{%- endif -%}
                                                {%- if image_1.width >= 2000 -%}{{ image_1 | image_url: width: 2000 }} 2000w,{%- endif -%}
                                                {%- if image_1.width >= 2800 -%}{{ image_1 | image_url: width: 2800 }} 2800w{%- endif -%}"
